<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.bigskysolutionsinc.boesen.spinrt_back.MainClassList"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="85dp"
        android:layout_height="18dp"
        android:layout_marginStart="16dp"
        android:text="@string/LastName"
        app:layout_constraintTop_toBottomOf="@+id/txtLastName"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="@+id/txtLastName" />

    <EditText
        android:id="@+id/txtPhone"
        android:layout_width="209dp"
        android:layout_height="41dp"
        android:layout_marginLeft="42dp"
        android:layout_marginStart="42dp"
        android:layout_marginTop="39dp"
        android:ems="10"
        android:inputType="phone"
        android:selectAllOnFocus="true"
        app:layout_constraintLeft_toRightOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/txtLastName" />

    <EditText
        android:id="@+id/txtFirstName"
        android:layout_width="144dp"
        android:layout_height="44dp"
        android:layout_marginLeft="13dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:hint="@string/FirstName"
        android:inputType="textPersonName"
        android:selectAllOnFocus="true"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="56dp"
        android:layout_height="42dp"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        android:text="@string/Bday"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="54dp"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <EditText
        android:id="@+id/txtLastName"
        android:layout_width="213dp"
        android:layout_height="42dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:hint="@string/LastName"
        android:inputType="textPersonName"
        android:selectAllOnFocus="true"
        app:layout_constraintLeft_toRightOf="@+id/txtFirstName"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/FirstName"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtFirstName"
        android:layout_marginStart="16dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:text="@string/Phone"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <Spinner
        android:id="@+id/spnMonth"
        android:layout_width="99dp"
        android:layout_height="34dp"
        android:entries="@array/Month"
        app:layout_constraintHorizontal_bias="0.262"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="34dp"
        app:layout_constraintTop_toBottomOf="@+id/txtPhone" />

    <EditText
        android:id="@+id/txtDay"
        android:layout_width="65dp"
        android:layout_height="40dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="170dp"
        android:ems="10"
        android:hint="@string/txtDay"
        android:inputType="number"
        android:selectAllOnFocus="true"
        app:layout_constraintHorizontal_bias="0.146"
        app:layout_constraintLeft_toRightOf="@+id/spnMonth"
        app:layout_constraintRight_toLeftOf="@+id/txtYear"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txtYear"
        android:layout_width="87dp"
        android:layout_height="38dp"
        android:layout_marginLeft="92dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="170dp"
        android:ems="10"
        android:hint="@string/txtYear"
        android:inputType="number"
        android:selectAllOnFocus="true"
        app:layout_constraintLeft_toLeftOf="@+id/txtDay"
        app:layout_constraintTop_toTopOf="parent" />

    <Switch
        android:id="@+id/swDegreeCert"
        android:layout_width="0dp"
        android:layout_height="41dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="73dp"
        android:checked="true"
        android:splitTrack="false"
        android:switchMinWidth="390dp"
        android:textOff="@string/cert"
        android:textOn="@string/twoYr"
        app:layout_constraintHorizontal_bias="0.666"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtDay"
        tools:checked="true"
        tools:ignore="MissingConstraints,RtlHardcoded"
        tools:text=" " />

    <Spinner
        android:id="@+id/spnDegree"
        android:layout_width="173dp"
        android:layout_height="30dp"
        android:entries="@array/lstMajor"
        android:spinnerMode="dropdown"
        android:layout_marginTop="6dp"
        app:layout_constraintTop_toBottomOf="@+id/swDegreeCert"
        tools:ignore="RtlHardcoded"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/lblDegree"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="39dp"
        android:layout_marginStart="39dp"
        android:text="@string/major"
        app:layout_constraintLeft_toLeftOf="@+id/spnDegree"
        app:layout_constraintTop_toBottomOf="@+id/spnDegree" />

    <Spinner
        android:id="@+id/spnCertificate"
        android:layout_width="176dp"
        android:layout_height="32dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:entries="@array/lstCertificate"
        android:spinnerMode="dialog"
        android:visibility="invisible"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/swDegreeCert"
        tools:ignore="RtlHardcoded" />

    <TextView
        android:id="@+id/lblCertificate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="25dp"
        android:layout_marginStart="25dp"
        android:text="@string/cert"
        android:visibility="invisible"
        app:layout_constraintLeft_toLeftOf="@+id/spnCertificate"
        app:layout_constraintTop_toBottomOf="@+id/spnCertificate" />

    <Button
        android:id="@+id/btnNext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:text="@string/btnNext"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/cert"
        android:layout_marginLeft="33dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/swDegreeCert"
        android:layout_marginStart="33dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/twoYr"
        app:layout_constraintLeft_toRightOf="@+id/textView5"
        android:layout_marginLeft="177dp"
        app:layout_constraintBottom_toTopOf="@+id/swDegreeCert"
        android:layout_marginStart="177dp" />

</android.support.constraint.ConstraintLayout>
